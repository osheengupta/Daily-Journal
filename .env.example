# Example .env file - Copy to .env and update with your actual credentials
# Twilio credentials - Get these from https://www.twilio.com/console
TWILIO_ACCOUNT_SID=your_account_sid_here  # Should start with "AC"
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number  # In E.164 format, e.g., +15551234567

# ElevenLabs credentials
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id_here

# Other configuration
PORT=8000  # Optional, defaults to 8000 if not specified
